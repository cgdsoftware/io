{
    "name": "cgdsoftware/io",
    "description": "IO process monitoring dependency for Laravel-Enso",
    "keywords": [
        "laravel",
        "cgdsoftware",
        "io"
    ],
    "homepage": "https://github.com/cgdsoftware/io",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://cgdsoftware.com",
            "role": "Developer"
        }
    ],
    "require": {
        "cgdsoftware/core": "^8.0",
        "cgdsoftware/enums": "^2.0"
    },
    "autoload": {
        "psr-4": {
